{
  "name": "api_levapp",
  "version": "1.0.0",
  "description": "API principal LEVAPP -- admin, loja, cliente e entregas",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx babel src --out-dir dist",
    "postbuild": "npx knex migrate:latest",
    "start": "node -r dotenv/config dist/server.js",
    "dev": "concurrently \"babel src --watch --out-dir dist\"  \"nodemon -r dotenv/config dist/server.js\"",
    "test-dev": "jest --watch --runInBand --coverage",
    "test": "jest --runInBand --coverage --detectOpenHandles"
  },
  "author": "joaop",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^26.0.1",
    "concurrently": "^5.2.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "sequelize-cli": "^5.5.1",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "moment": "^2.25.3",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.7",
    "uuid": "^8.0.0",
    "yup": "^0.28.5"
  },
  "engines": {
    "node": "10.x"
  }
}
